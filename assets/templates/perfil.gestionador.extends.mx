<template id="v-perfil">
    <div class="card card-profile-interest">
        <div class="card-header pd-y-15 pd-x-20 d-flex align-items-center justify-content-between">
            <h6 class="tx-uppercase tx-semibold mg-b-0">
                Mi Perfil
            </h6>
        </div>
        <!-- card-header -->
        <div class="card-body pd-25">
            <div class="row">
                <div class="col-sm col-lg-12 col-xl">
                    <div class="media">
                        <div class="wd-45 ht-45 bg-primary rounded d-flex align-items-center justify-content-center">
                            <i class="icon ion-md-person tx-white-7">
                            </i>
                        </div>
                        <div class="media-body pd-l-25">
                            <h6 class="tx-color-01 mg-b-5">
                                Datos Personales
                            </h6>
                            <p class="tx-12 mg-b-10">
                                Actualiza tu infromación personal.
                                <a href="perfil#!/%perfil_id%/datos-personales">
                                    Ver Más
                                </a>
                            </p>
                        </div>
                    </div>
                    <!-- media -->
                    <div class="media">
                        <div class="wd-45 ht-45 bg-primary rounded d-flex align-items-center justify-content-center">
                            <i class="icon ion-md-calendar tx-white-7">
                            </i>
                        </div>
                        <div class="media-body pd-l-25">
                            <h6 class="tx-color-01 mg-b-5">
                                Timeline
                            </h6>
                            <p class="tx-12 mg-b-10">
                                Historial de Actividad
                                <a href="perfil#!/%perfil_id%/timeline">
                                    Ver Más
                                </a>
                            </p>
                        </div>
                    </div>
                    <!-- media -->
                </div>
                <!-- col -->
                <div class="col-sm col-lg-12 col-xl mg-t-25 mg-sm-t-0 mg-lg-t-25 mg-xl-t-0">
                    <div class="media">
                        <div class="wd-45 ht-45 bg-primary rounded d-flex align-items-center justify-content-center">
                            <i class="icon ion-md-key tx-white-7">
                            </i>
                        </div>
                        <div class="media-body pd-l-25">
                            <h6 class="tx-color-01 mg-b-5">
                                Datos de Convenio
                            </h6>
                            <p class="tx-12 mg-b-10">
                                Actualiza toda la infromación de Convenio
                                <a href="perfil#!/%perfil_convenio%/convenio">
                                    Ver Más
                                </a>
                            </p>
                        </div>
                    </div>
                    <!-- media -->
                </div>
                <!-- col -->
            </div>
            <!-- row -->
        </div>
        <!-- card-body -->
    </div>
    <!-- card -->
</template>
<template id="v-datos-personales">
    <div class="card card-profile-interest">
        <div class="card-header pd-y-15 pd-x-20 d-flex align-items-center justify-content-between">
            <h6 class="tx-uppercase tx-semibold mg-b-0">
                Datos Personales
            </h6>
        </div>
        <!-- card-header -->
        <div class="card-body pd-25">
            <div class="row">
                <div class="col-sm-12 col-lg-12 col-xl-12">
                    <form>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="inputEmail4">
                                    Correo Electrónico
                                </label>
                                <input class="form-control" disabled="" id="email" placeholder="Correo Electrónico" type="email" value="%email%">
                                </input>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputPassword4">
                                    Usuario
                                </label>
                                <input class="form-control" disabled="" id="user" placeholder="Usuario" type="text" value="%user%">
                                </input>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="inputEmail4">
                                    Nombres
                                </label>
                                <input class="form-control" disabled="" id="nombres" placeholder="Nombres" type="text" value="%nombres%">
                                </input>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputEmail4">
                                    Apellidos
                                </label>
                                <input class="form-control" disabled="" id="apellidos" placeholder="Apellidos" type="text" value="%apellidos%">
                                </input>
                            </div>
                        </div>
                        <button class="btn btn-primary editar" id="%id%">
                            Editar
                        </button>
                        <button class="btn btn-primary cambiar-contraseña" id="%id%">
                            Cambiar Contraseña
                        </button>
                    </form>
                </div>
                <!-- col -->
            </div>
            <!-- row -->
        </div>
        <!-- card-body -->
    </div>
</template>
<template id="v-convenio">
    <div class="card card-profile-interest">
        <div class="card-header pd-y-15 pd-x-20 d-flex align-items-center justify-content-between">
            <h6 class="tx-uppercase tx-semibold mg-b-0">
                Datos de Convenio
            </h6>
        </div>
        <!-- card-header -->
        <div class="card-body pd-25">
            <div class="row">
                <div class="col-sm-12 col-lg-12 col-xl-12">
                    <form>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="inputEmail4">
                                    Isologo
                                </label>
                                <br/>
                                <img class="img-thumbnail rounded " id="temp_isologo" src="%isologo%">
                                </img>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputAddress2">
                                Dirección
                            </label>
                            <input class="form-control" disabled="" id="dir" name="dir" type="text" value="%dir%">
                            </input>
                        </div>
                        <div class="form-group">
                            <label for="inputAddress2">
                                Telefonos:
                            </label>
                            <input class="form-control" disabled="" id="tel" name="tel" type="text" value="%tel%">
                            </input>
                        </div>
                        <div class="form-group">
                            <label for="inputAddress2">
                                Correo electrónco:
                            </label>
                            <input class="form-control" disabled="" id="email" name="email" type="email" value="%email%">
                            </input>
                        </div>
                    </form>
                </div>
                <!-- col -->
            </div>
            <!-- row -->
        </div>
        <!-- card-body -->
    </div>
</template>
<template id="v-timeline">
    <div class="card card-profile-interest">
        <div class="card-header pd-y-15 pd-x-20 d-flex align-items-center justify-content-between">
