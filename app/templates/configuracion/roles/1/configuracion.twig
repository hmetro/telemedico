{% extends 'overall/layout' %}
{% block appMetaHead %}
<title>
    Configuración | {{config.build.name}}
</title>
{% endblock %}
{% block appHead %}
<!-- DashForge CSS -->
<link href="assets/dashforge/assets/css/dashforge.profile.css" rel="stylesheet"/>
{% endblock %}
{% block appHeader %}
{% include 'overall/header' %}
{% endblock %}
{% block appBody %}
<div class="content content-fixed content-profile">
    <div class="container pd-x-0 pd-lg-x-10 pd-xl-x-0">
        <div class="media d-block d-lg-flex">
            <div class="media-body mg-t-40 mg-lg-t-0 pd-lg-x-10">
                <div class="card card-profile-interest">
                    <div class="card-header pd-y-15 pd-x-20 d-flex align-items-center justify-content-between">
                        <h6 class="tx-uppercase tx-semibold mg-b-0">
                            Configuración
                        </h6>
                    </div>
                    <!-- card-header -->
                    <div class="card-body pd-25">
                        <div class="row">
                            <div class="col-sm col-lg-12 col-xl">
                                <div class="media">
                                    <div class="wd-45 ht-45 bg-primary rounded d-flex align-items-center justify-content-center">
                                        <i class="tx-white-7 wd-20 ht-20" data-feather="users">
                                        </i>
                                    </div>
                                    <div class="media-body pd-l-25">
                                        <h6 class="tx-color-01 mg-b-5">
                                            Pacientes
                                        </h6>
                                        <p class="tx-12 mg-b-10">
                                            Configura las diferentes categorias de tus pacientes.
                                            <br/>
                                            <a href="{{config.build.url}}configuracion/pacientes">
                                                Ver Mas
                                            </a>
                                        </p>
                                        <span class="tx-12 tx-color-03">
                                            Últimos cambios: 12/12/2020
                                        </span>
                                    </div>
                                </div>
                                <!-- media -->
                                <div class="media">
                                    <div class="wd-45 ht-45 bg-warning rounded d-flex align-items-center justify-content-center">
                                        <i class="tx-white-7 wd-20 ht-20" data-feather="calendar">
                                        </i>
                                    </div>
                                    <div class="media-body pd-l-25">
                                        <h6 class="tx-color-01 mg-b-5">
                                            Citas
                                        </h6>
                                        <p class="tx-12 mg-b-10">
                                            Configura los tipos de citas: sus horarios y tiempos.
                                            <br/>
                                            <a href="{{config.build.url}}configuracion/citas">
                                                Ver Mas
                                            </a>
                                        </p>
                                        <span class="tx-12 tx-color-03">
                                            Últimos cambios: 12/12/2020
                                        </span>
                                    </div>
                                </div>
                                <!-- media -->
                            </div>
                            <!-- col -->
                            <div class="col-sm col-lg-12 col-xl mg-t-25 mg-sm-t-0 mg-lg-t-25 mg-xl-t-0">
                                <div class="media">
                                    <div class="wd-45 ht-45 bg-primary rounded d-flex align-items-center justify-content-center">
                                        <i class="tx-white-7 wd-20 ht-20" data-feather="users">
                                        </i>
                                    </div>
                                    <div class="media-body pd-l-25">
                                        <h6 class="tx-color-01 mg-b-5">
                                            Usuarios
                                        </h6>
                                        <p class="tx-12 mg-b-10">
                                            Configura los accesos de tus usuarios.
                                            <br/>
                                            <a href="{{config.build.url}}configuracion/usuarios">
                                                Ver Mas
                                            </a>
                                        </p>
                                        <span class="tx-12 tx-color-03">
                                            Últimos cambios: 12/12/2020
                                        </span>
                                    </div>
                                </div>
                                <!-- media -->
                                <div class="media">
                                    <div class="wd-45 ht-45 bg-pink rounded d-flex align-items-center justify-content-center">
                                        <i class="tx-white-7 wd-20 ht-20" data-feather="briefcase">
                                        </i>
                                    </div>
                                    <div class="media-body pd-l-25">
                                        <h6 class="tx-color-01 mg-b-5">
                                            Facturación y Pagos
                                        </h6>
                                        <p class="tx-12 mg-b-10">
                                            Gestiona tus datos de facturación y pagos.
                                            <br/>
                                            <a href="{{config.build.url}}configuracion/facturacion">
                                                Ver Mas
                                            </a>
                                        </p>
                                        <span class="tx-12 tx-color-03">
                                            Últimos cambios: 12/12/2020
                                        </span>
                                    </div>
                                </div>
                                <!-- media -->
                            </div>
                            <!-- col -->
                        </div>
                        <!-- row -->
                    </div>
                    <!-- card-body -->
                </div>
                <!-- card -->
            </div>
            <!-- media-body -->
            <div class="profile-sidebar mg-t-40 mg-lg-t-0 pd-lg-l-25">
