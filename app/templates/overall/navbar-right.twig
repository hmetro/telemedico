</div>
<!-- navbar-right -->
<template id="v-notify">
    <a class="dropdown-item" href="#">
        <div class="media">
            <div class="avatar avatar-sm avatar-online">
                <img alt="" class="rounded-circle" src="https://via.placeholder.com/350"/>
            </div>
            <div class="media-body mg-l-15">
                <p>
                    %message%
                </p>
                <span id="n-timestamp">
                    %timestamp%
                </span>
            </div>
            <!-- media-body -->
        </div>
        <!-- media -->
    </a>
</template>
