<template id="btn-table-opc">
    <div class="btn-group" role="group">
        <button class="btn btn-xs btn-outline-primary ver-resultado" id="%id%" type="button">
            <i class="icon ion-md-document">
            </i>
            Ver
        </button>
        <button class="btn btn-xs btn-outline-primary descargar-resultado" id="%id%" type="button">
            <i class="icon ion-md-download">
            </i>
            Descargar
        </button>
        <button class="btn btn-xs btn-outline-primary enviar-resultado" id="%id%" title="Enviar por Correo Electrónico" type="button">
            <i class="icon ion-md-mail">
            </i>
            Enviar
        </button>
    </div>
</template>
<template id="v-ver-resultado">
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">
                RESULTADO DE LABORATORIO:
            </h5>
            <h6 class="card-subtitle mb-2 text-muted">
                <b>
                    PACIENTE:
                </b>
                %paciente%
            </h6>
            <h6 class="card-subtitle mb-2 text-muted">
                <b>
                    ORIGEN:
                </b>
                %origen%
            </h6>
            <h6 class="card-subtitle mb-2 text-muted">
                <b>
                    FECHA:
                </b>
                %fecha%
            </h6>
            <h6 class="card-subtitle mb-2 text-muted">
                <b>
                    CONVENIO:
                </b>
                %convenio%
            </h6>
            <div class="divider-text">
                <a class="card-link mg-r-20 tx-12" href="#" id="prev">
                    <i class="icon ion-md-return-left">
                    </i>
                    Anterior
                </a>
                <span class="tx-12">
                    Página:
                    <span id="page_num">
                    </span>
                    /
                    <span id="page_count">
                    </span>
                </span>
                <a class="card-link mg-l-20 tx-12" href="#" id="next">
                    Siguiente
                    <i class="icon ion-md-return-right">
                    </i>
                </a>
                <a class="card-link mg-l-80 tx-12" download="Res_Lab_%id_resultado%_%fecha%_.pdf" href="%pdf%">
                    <i class="icon ion-md-download">
                    </i>
                    Descargar
                </a>
                <a class="card-link mg-l-120 tx-12" download="Res_Lab_%id_resultado%_%fecha%_.pdf" href="%pdf%" title="Enviar por Correo Electrónico">
                    <i class="icon ion-md-mail">
                    </i>
                    Enviar
                </a>
            </div>
            <div class="d-flex justify-content-center">
                <canvas id="the-canvas" width="100%">
                </canvas>
            </div>
        </div>
    </div>
</template>
<template id="v-table">
    <table class="table table-sm" id="table">
    </table>
</template>
<template id="v-spinner">
    <div class="card">
        <div class="card-body">
            <div class="text-center pd-y-10 pd-x-20 bg-ui-01" style="height: 1000px;">
                <span aria-hidden="true" class="spinner-border spinner-border-sm mg-t-25" role="status">
                </span>
                Procesando...
            </div>
        </div>
    </div>
</template>
<template id="v-enviar-resultado">
    <fieldset class="form-fieldset">
        <legend>
            Personal Information
        </legend>
        <form>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label" for="inputEmail3">
                    Email
                </label>
                <div class="col-sm-10">
                    <input class="form-control" id="inputEmail3" placeholder="Email" type="email">
                    </input>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label" for="inputPassword3">
                    Password
                </label>
                <div class="col-sm-10">
                    <input class="form-control" id="inputPassword3" placeholder="Password" type="password">
                    </input>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-form-label col-sm-2 pt-0">
                    Radios
                </label>
                <div class="col-sm-10">
                    <div class="custom-control custom-radio">
                        <input checked="" class="custom-control-input" name="customRadio" type="radio">
                            <label class="custom-control-label">
                                First radio
                            </label>
                        </input>
                    </div>
                    <div class="custom-control custom-radio">
                        <input class="custom-control-input" name="customRadio" type="radio">
                            <label class="custom-control-label">
                                Second radio
                            </label>
                        </input>
                    </div>
                    <div class="custom-control custom-radio">
                        <input class="custom-control-input" disabled="" name="customRadio" type="radio">
                            <label class="custom-control-label">
                                Third disabled radio
                            </label>
                        </input>
                    </div>
                </div>
            </div>
            <div class="form-group row">
                <div class="col-sm-2">
                    Checkbox
                </div>
                <div class="col-sm-10">
                    <div class="custom-control custom-checkbox">
                        <input class="custom-control-input" type="checkbox">
                            <label class="custom-control-label">
                                Example checkbox
                            </label>
                        </input>
                    </div>
                </div>
            </div>
            <div class="form-group row mg-b-0">
                <div class="col-sm-10">
                    <button class="btn btn-primary" type="submit">
                        Submit Form
                    </button>
                </div>
            </div>
        </form>
    </fieldset>
</template>
<br/>
