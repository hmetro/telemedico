    var epCitasAgendaPendientes = '{{config.apiHm.baseUrl}}{{config.apiHm.medicos.citasAgendaPendientes}}';
    var epCitasAgendaPasadas = '{{config.apiHm.baseUrl}}{{config.apiHm.medicos.citasAgendaPasadas}}';
    var epCitasNombres = '{{config.apiHm.baseUrl}}{{config.apiHm.medicos.citasNombres}}';
